{
  "name": "v4-sujeet-pro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run clean && astro dev",
    "prod": "npm run clean && astro build && npm run preview",
    "dev:gh": "astro dev -- --site https://projects.sujeet.pro --base /v4.sujeet.pro/",
    "build:gh": "astro build -- --site https://projects.sujeet.pro --base /v4.sujeet.pro/",
    "build": "astro build",
    "setup": "node --experimental-strip-types scripts/setup.ts",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "lint": "eslint --fix .",
    "format": "prettier . --write --ignore-unknown",
    "sync": "astro sync",
    "clean": "rm -rf .astro dist",
    "check-unused-images": "tsx scripts/check-unused-images.ts",
    "remove-unused-images": "tsx scripts/remove-unused-images.ts",
    "all": "npm run setup && npm run build && npm run check-unused-images && npm run remove-unused-images",
    "test": "vitest run",
    "deploy": "npm run build && wrangler deploy",
    "deploy:dry-run": "npm run build && wrangler deploy --dry-run",
    "dev:worker": "wrangler dev"
  },
  "devDependencies": {
    "@eslint/css": "^0.10.0",
    "@eslint/js": "^9.31.0",
    "@eslint/json": "^0.13.0",
    "@eslint/markdown": "^7.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.14",
    "eslint": "^9.31.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^16.3.0",
    "json5": "^2.2.3",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "remark-code-import": "^1.2.0",
    "remark-inline-svg": "^1.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.25.0"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@expressive-code/plugin-collapsible-sections": "^0.41.3",
    "@expressive-code/plugin-line-numbers": "^0.41.3",
    "@fontsource-variable/open-sans": "^5.2.6",
    "@iconify-json/carbon": "^1.2.10",
    "@iconify-json/devicon": "^1.2.31",
    "@iconify-json/devicon-plain": "^1.2.30",
    "@types/glob": "^9.0.0",
    "astro": "^5.12.0",
    "astro-expressive-code": "^0.41.3",
    "astro-icon": "^1.1.5",
    "astro-pagefind": "^1.8.3",
    "daisyui": "^5.0.46",
    "glob": "^11.0.3",
    "mdast-util-to-string": "^4.0.0",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-mermaid": "^3.0.0",
    "rehype-slug": "^6.0.0",
    "remark-collapse": "^0.1.2",
    "remark-emoji": "^5.0.1",
    "remark-math": "^6.0.0",
    "remark-normalize-headings": "^4.0.0",
    "remark-toc": "^9.0.0",
    "tailwindcss": "^4.1.11"
  }
}
