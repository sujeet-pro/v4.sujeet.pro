{
  "name": "v4-sujeet-pro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "npx playwright install chromium",
    "dev": "npm run clean && astro dev",
    "build": "npm run clean && astro build",
    "preview": "astro preview",
    "prod": "npm run clean && astro build && npm run preview",
    "copy:katex": "node --experimental-strip-types scripts/copy-katex-assets.ts",
    "astro": "astro",
    "check": "astro check",
    "lint": "eslint --fix .",
    "format": "prettier . --write --ignore-unknown --loglevel error",
    "sync": "astro sync",
    "clean": "rm -rf .astro dist",
    "all": "npm run copy:katex && npm run check && npm run lint && npm run format && npm run validate:content && npm run build && npm run validate:build && npm run preview",
    "test": "vitest run",
    "validate:build": "npx tsx scripts/validation/validate-static-urls.ts",
    "validate:content:h1": "npx tsx scripts/validation/validate-content-h1.ts",
    "validate:content:ordering": "npx tsx scripts/validation/validate-content-ordering.ts",
    "validate:content:home": "npx tsx scripts/validation/validate-content-home.ts",
    "validate:content:vanity": "npx tsx scripts/validation/validate-content-vanity.ts",
    "validate:content:links": "npx tsx scripts/validation/validate-content-links.ts",
    "validate:content": "npx tsx scripts/validation/validate-content.ts",
    "validate:local": "npx tsx scripts/validation/validate-live-site.ts http://localhost:4322",
    "validate:prod": "npx tsx scripts/validation/validate-live-site.ts https://sujeet.pro",
    "validate:hosted": "npm run validate:local && npm run validate:prod",
    "validate": "npx tsx scripts/validation/validate.ts",
    "validate:all": "npm run validate",
    "external:revalidate": "npx tsx scripts/validation/validate-external-links.ts --mode failed",
    "external:revalidate:all": "npx tsx scripts/validation/validate-external-links.ts --mode all",
    "lighthouse": "unlighthouse --site https://sujeet.pro",
    "lighthouse:local": "unlighthouse --site http://localhost:4321",
    "lighthouse:desktop": "unlighthouse --site https://sujeet.pro --desktop"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.8",
    "@unlighthouse/cli": "^0.17.4",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^17.0.0",
    "playwright": "^1.57.0",
    "prettier": "3.7.4",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "puppeteer": "^24.35.0",
    "remark-code-import": "^1.2.0",
    "remark-inline-svg": "^1.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.1",
    "@expressive-code/plugin-collapsible-sections": "^0.41.5",
    "@expressive-code/plugin-line-numbers": "^0.41.5",
    "@iconify-json/carbon": "^1.2.18",
    "@iconify-json/devicon": "^1.2.56",
    "@iconify-json/devicon-plain": "^1.2.51",
    "@orama/orama": "^3.1.7",
    "@types/glob": "^9.0.0",
    "astro": "5.17",
    "astro-expressive-code": "^0.41.5",
    "astro-icon": "^1.1.5",
    "glob": "^13.0.0",
    "json5": "^2.2.3",
    "mdast-util-to-string": "^4.0.0",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-mermaid": "^3.0.0",
    "remark-emoji": "^5.0.2",
    "remark-math": "^6.0.0",
    "remark-normalize-headings": "^4.0.0",
    "svg-pan-zoom": "^3.6.2",
    "tailwindcss": "^4.1.18"
  },
  "volta": {
    "node": "24.12.0",
    "npm": "11.7.0"
  }
}
