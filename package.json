{
  "name": "v4-sujeet-pro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "postinstallx": "npm install -D @playwright/test@latest &&npx playwright install --with-deps",
    "dev": "npm run clean && astro dev",
    "prod": "npm run clean && astro build && npm run preview",
    "dev:cf": "astro dev -- --site https://sujeet.pro --base /",
    "build:cf": "astro build -- --site https://sujeet.pro --base /",
    "build": "astro build",
    "setup": "node --experimental-strip-types scripts/setup.ts",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "lint": "eslint --fix .",
    "format": "prettier . --write --ignore-unknown",
    "sync": "astro sync",
    "clean": "rm -rf .astro dist",
    "check-unused-images": "tsx scripts/check-unused-images.ts",
    "remove-unused-images": "tsx scripts/remove-unused-images.ts",
    "all": "npm run setup && npm run build && npm run check-unused-images && npm run remove-unused-images",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/css": "^0.9.0",
    "@eslint/js": "^9.29.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.5.0",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.0.13",
    "eslint": "^9.29.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^16.2.0",
    "json5": "^2.2.3",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "0.6.12",
    "remark-code-import": "^1.2.0",
    "remark-inline-svg": "^1.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@expressive-code/plugin-collapsible-sections": "^0.41.2",
    "@expressive-code/plugin-line-numbers": "^0.41.2",
    "@fontsource-variable/open-sans": "^5.2.6",
    "@iconify-json/carbon": "^1.2.9",
    "@iconify-json/devicon": "^1.2.29",
    "@iconify-json/devicon-plain": "^1.2.28",
    "@types/glob": "^8.1.0",
    "astro": "^5.12.0",
    "astro-expressive-code": "^0.41.2",
    "astro-icon": "^1.1.5",
    "astro-pagefind": "^1.8.3",
    "daisyui": "^5.0.43",
    "glob": "^11.0.3",
    "mdast-util-to-string": "^4.0.0",
    "playwright": "^1.53.1",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-mermaid": "^3.0.0",
    "rehype-slug": "^6.0.0",
    "remark-collapse": "^0.1.2",
    "remark-emoji": "^5.0.1",
    "remark-math": "^6.0.0",
    "remark-normalize-headings": "^4.0.0",
    "remark-toc": "^9.0.0",
    "tailwindcss": "^4.1.10"
  }
}
