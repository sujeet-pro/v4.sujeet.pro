{
  "name": "v4-sujeet-pro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run clean && astro dev",
    "dev:cf": "npm run clean && astro dev",
    "dev:gh": "npm run clean && astro dev --base /v4.sujeet.pro/",
    "build": "astro build",
    "build:cf": "npm run clean && astro build",
    "build:gh": "npm run clean && astro build --site https://projects.sujeet.pro --base /v4.sujeet.pro/",
    "preview": "astro preview",
    "preview:cf": "astro preview",
    "preview:gh": "astro preview --base /v4.sujeet.pro/",
    "prod": "npm run clean && astro build && npm run preview",
    "setup": "node --experimental-strip-types scripts/setup.ts",
    "astro": "astro",
    "check": "astro check",
    "lint": "eslint --fix .",
    "format": "prettier . --write --ignore-unknown",
    "sync": "astro sync",
    "clean": "rm -rf .astro dist",
    "all": "npm run setup && npm run build && npm run check-unused-images && npm run remove-unused-images",
    "test": "vitest run",
    "validate:static": "npx tsx scripts/validation/validate-static.ts",
    "validate:live": "npx tsx scripts/validation/validate-live.ts",
    "deploy": "npm run build:cf && wrangler deploy",
    "deploy:dry-run": "npm run build:cf && wrangler deploy --dry-run",
    "dev:worker": "wrangler dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.8",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^17.0.0",
    "playwright": "^1.57.0",
    "prettier": "3.7.4",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "remark-code-import": "^1.2.0",
    "remark-inline-svg": "^1.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17",
    "wrangler": "^4.59.1"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.1",
    "@expressive-code/plugin-collapsible-sections": "^0.41.5",
    "@expressive-code/plugin-line-numbers": "^0.41.5",
    "@iconify-json/carbon": "^1.2.18",
    "@iconify-json/devicon": "^1.2.56",
    "@iconify-json/devicon-plain": "^1.2.51",
    "@orama/orama": "^3.1.7",
    "@types/glob": "^9.0.0",
    "astro": "^5.16.9",
    "astro-expressive-code": "^0.41.5",
    "astro-icon": "^1.1.5",
    "glob": "^13.0.0",
    "mdast-util-to-string": "^4.0.0",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-mermaid": "^3.0.0",
    "rehype-slug": "^6.0.0",
    "remark-collapse": "^0.1.2",
    "remark-emoji": "^5.0.2",
    "remark-math": "^6.0.0",
    "remark-normalize-headings": "^4.0.0",
    "remark-toc": "^9.0.0",
    "tailwindcss": "^4.1.18"
  },
  "volta": {
    "node": "24.12.0",
    "npm": "11.7.0"
  }
}
